<div class="posts-wrapper row">
  <% @posts.each do |post| %>
  <%if post.user == current_user %>
    <div class="post">
      <div class="post-head">
        <div class="name">
          <%= post.user.user_name %>
        </div>
      </div>
      <div class="image center-block">
        <%= link_to (image_tag post.image.url(:medium), class:'img-responsive'), post_path(post) %>
        	
        </div>
      <p class="caption">
        <%= post.caption %>
      </p>
      <%= link_to 'Show Post', post_path(post) %>
    </div>
    <% end %>
  <% end %>
</div>
